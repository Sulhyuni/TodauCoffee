<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
 <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
<script type="text/javascript">

$(function(){
	$("#loginBtn").click(function(){
		location="/member/login.do";
	});
	
	$("#findPw").click(function(){
		location="/member/findPw.do";
	});
	
});

</script>
</head>
<body>
<div class="container">
	<h1 style="font-size: xx-large;">찾은 아이디 확인</h1>
	<table class="table" style="box-sizing: border-box;">
		<tr>
			<th style="font-size: x-large;">아이디:</th>
			<td style="font-size: x-large;">${id }</td>
		</tr>
	</table>
	
	<button  id="loginBtn">로그인</button>
	<button  id="findPw">비밀번호 찾기</button>
	
</div>
</body>
</html>